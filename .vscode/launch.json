{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: WOG",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/wog/main.py",
            "cwd": "${workspaceFolder}/src/wog",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src/wog"
            }
        },
        {
            "name": "Python Debugger: Score Site",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/score_site/main_score.py",
            "cwd": "${workspaceFolder}/src/score_site",
            "env": {
                "PYTHONPATH": "${workspaceFolder}/src/score_site"
            }
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}